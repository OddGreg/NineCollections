{
    "name"             : "oddgreg/nine-collections",
    "description"      : "A library of Collection classes and functions.",
    "type"             : "library",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "autoload"         : {
        "classmap"             : [
            "tests",
            "src"
        ],
        "files"                : [
            "src/helpers.php"
        ],
        "exclude-from-classmap": [
        ],
        "psr-4"                : {
            "Nine\\Collections\\": "src",
            "Nine\\Structure\\"  : "src/Structure/src"
        }
    },
    "minimum-stability": "dev",
    "require"          : {
        "oddgreg/nine-library": "dev-master",
        "vlucas/phpdotenv"    : "^2.3"
    },
    "prefer-stable"    : true,
    "-require-dev"      : {
        "phpunit/phpunit"                    : "^5.1@dev",
        "psy/psysh"                          : "@stable",
        "tracy/tracy"                        : "^2.4@dev",
        "codeclimate/php-test-reporter"      : "dev-master",
        "phpmd/phpmd"                        : "^2.4",
        "mockery/mockery": "~0.9.4"
    }
}
